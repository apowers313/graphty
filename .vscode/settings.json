{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "eslint.enable": true,
    "eslint.format.enable": true,
    "eslint.run": "onType",
    "eslint.alwaysShowStatus": true,
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "javascript.format.enable": false,
    "typescript.format.enable": false
}